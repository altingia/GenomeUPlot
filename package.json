{
  "name": "GenomeUPlot",
  "version": "0.0.1",
  "description": "GenomeUPlot",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dist && babel-node src/server --dist",
    "start-dev": "webpack-dev-server --profile --inline --colors",
    "lint": "eslint src webpack.config.babel.js --ext .js",
    "test": "yarn lint && jest --coverage",
    "precommit": "yarn test",
    "build": "rimraf build && webpack --progress --profile --colors",
    "dist": "rimraf dist && cross-env NODE_ENV=production webpack --progress --profile"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "3.3.6",
