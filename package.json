    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.4.2"
