    "express": "^4.16.2",
    "jquery": "2.2.0",
    "jquery-ui": "1.12.1",
    "jquery.scrollto": "^2.1.2",
    "save-svg-as-png": "^1.2.0",

    "svg-intersections": "^0.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.2.1",
    "common-tags": "^1.4.0",
    "concurrently": "^3.4.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
